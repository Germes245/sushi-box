<h1>Классические роллы</h1>
<div class="rols">
  <% for(let i of catalog){ %>
  <div class="card_for_rols">
    <div class="hit_and_new">
      <div class="for_hit_and_new orange">ХИТ</div>
      <div class="for_hit_and_new blue">NEW</div>
    </div>
    <div class="img sushi"></div>
    <div class="name_of_product">
      <span><%= i.name %></span>
      <div id="container_for_upper_ungredients">
        <div class="mark">
          <div
            class="pict"
            style="background-image: url(&quot;./assets/image\ 514.png&quot;)"
          ></div>
          <span>Скидка</span>
        </div>
        <div class="mark">
          <div
            class="pict"
            style="background-image: url(&quot;./assets/image\ 1565.png&quot;)"
          ></div>
          <span>Вега</span>
        </div>
      </div>
    </div>
    <div class="text_for_card">
      <%= i.description %>
    </div>
    <div class="price">
      <strike>200 ₽</strike>
      <span><%= i.cost %></span>
    </div>
  </div>
  <% } %>
</div>
